{
  "name": "simple-node-starter",
  "version": "0.0.2",
  "description": "Welcome to the simple node starter.",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server/server.js",
    "dev": "NODE_ENV=development nodemon server/server.js",
    "build:webpack": "webpack --progress --colors",
    "build:webpack:dev": "webpack --watch --progress --colors",
    "build": "rm -rf node_modules && npm install && npm run build:webpack",
    "build:dev": "npm install && npm install -g nodemon && npm run build:webpack:dev"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anthonyjgrove/node-simple-starter.git"
  },
  "keywords": [
    "node",
    "starter",
    "pack",
    "nodejs",
    "react"
  ],
  "author": "Anthony Grove",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anthonyjgrove/node-simple-starter/issues"
  },
  "homepage": "https://github.com/anthonyjgrove/node-simple-starter#readme",
  "dependencies": {
    "babel-preset-stage-1": "^6.1.18",
    "body-parser": "^1.14.2",
    "compression": "^1.6.1",
    "connect-mongo": "^1.1.0",
    "cookie-parser": "^1.4.1",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "express-flash": "0.0.2",
    "express-session": "^1.13.0",
    "hogan-express": "^0.5.2",
    "lodash": "^3.10.1",
    "method-override": "^2.3.5",
    "mongoose": "4.4.1",
    "morgan": "^1.6.1",
    "node-sass-middleware": "^0.9.7",
    "passport": "^0.3.2",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0-rc5",
    "redux": "^3.0.4",
    "redux-promise": "^0.5.1"
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "webpack": "^1.12.9"
  }
}
