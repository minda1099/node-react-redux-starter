{
  "name": "node-react-redux-starter",
  "version": "0.0.5",
  "description": "Welcome to the Node React Redux Starter.",
  "main": "server.js",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "prod": "NODE_ENV=production node server/server.js",
    "dev": "NODE_ENV=development nodemon server/server.js",
    "test:app": "karma start test/app.karma.conf.js",
    "test:server": "NODE_ENV=test; mocha `find server -type f -name '*-test.js'`",
    "lint": "eslint ./ || true",
    "build:webpack": "webpack -p --config ./server/config/webpack.prod.config.js",
    "build": "rm -rf node_modules && rm -rf public && npm install && npm run build:webpack"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anthonyjgrove/node-react-redux-starter.git"
  },
  "keywords": [
    "node",
    "starter",
    "pack",
    "nodejs",
    "react"
  ],
  "author": "Anthony Grove",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anthonyjgrove/node-simple-starter/issues"
  },
  "homepage": "https://github.com/anthonyjgrove/node-react-redux-starter#readme",
  "dependencies": {
    "asyncawait": "^1.0.4",
    "axios": "^0.12.0",
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.15.1",
    "compression": "^1.6.2",
    "express": "^4.13.4",
    "fbgraph": "^1.2.0",
    "glob": "^7.0.3",
    "googleapis": "^7.1.0",
    "history": "^3.0.0",
    "http-proxy": "^1.13.3",
    "immutable": "^3.8.1",
    "jsonwebtoken": "^7.0.0",
    "jwt-decode": "^2.0.1",
    "lodash": "^4.13.1",
    "method-override": "^2.3.6",
    "mongoose": "4.4.19",
    "morgan": "^1.7.0",
    "node-sass-middleware": "^0.9.8",
    "react": "^15.1.0",
    "react-addons-shallow-compare": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-facebook-login": "https://github.com/anthonyjgrove/react-facebook-login.git",
    "react-google-login": "^2.0.2",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
